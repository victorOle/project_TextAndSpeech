        <infon key="type">Chemical</infon>
        <infon key="identifier">MESH:D005978</infon>
        <location offset="2384" length="3"/>
        <text>GSH</text>
      </annotation>
      <annotation id="C26">
        <infon key="type">Chemical</infon>
        <infon key="identifier">MESH:D008623</infon>
        <location offset="2426" length="4"/>
        <text>β-ME</text>
      </annotation>
      <annotation id="C22">
